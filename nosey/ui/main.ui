   <string notr="true">QWidget {
	selection-background-color: #c1d5db;
	selection-color:#000;
}

QPushButton {
    border-radius: 5px;
	border: 1px solid transparent;
    padding: 6px;
 	background-color:#dedede;
}

QPushButton:hover {
    border: 1px solid #c1d5db;
}

QDockWidget::title {
    text-align: left; /* align the text to the left */
    background:#cedbdb ;
    padding-left: 5px;
	border-top-left-radius:5px;
	border-top-right-radius:5px;
}


QPushButton:pressed {
    background-color:#c1d5db;
 	border: 1px solid #c1d5db;
	border-style: inset;
}


QProgressBar {
  background-color:#9c9c9c;;
	height: 3px;
  border-radius: 2px;
  text-align: center;
	font-size : 1pt;
}


QProgressBar::chunk {
  background-color:  #787878;
  border-radius: 2px;
}


QComboBox {
    border-radius: 5px;
    padding: 2px;
 	background-color:#dedede;
}

QComboBox::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-left-width: 0px;
  border-left-color: #32414B;
  border-left-style: solid;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
  image: url(&quot;:/qss_icons/rc/arrow_down_disabled.png&quot;);
  height: 12px;
  width: 12px;
}


QWidget#plotFrameWidget,
QWidget#plotFrameWidget_rxes,
QWidget#plotFrameWidget_iad,
QWidget#plotFrameWidget_difference {
	border-radius: 5px;
	background-color:#fff;
	border: 1px solid transparent;
}

QWidget#plotFrameWidget:hover,
QWidget#plotFrameWidget_iad:hover,
QWidget#plotFrameWidget_difference:hover {
	border: 1px solid #dbdbdb;
}


QWidget#monitorFrameWidget {
	border-radius: 5px;
	background-color:#000;
	border: 1px solid transparent;
}

QWidget#plotWidgetRXES {
	border-radius: 5px;
	background-color:#fff;
	border: 1px solid transparent;
}

QWidget#monitorFrameWidget:hover {
	border: 1px solid transparent;
}



QTableView,
QTreeWidget
 {
  background-color: #f7f7f7;
  border: 1px solid #f7f7f7;
  color: #000;
  gridline-color: #f0f0f0;
  border-radius:5px;
font-size:8pt;
}


QTableView:hover {
 	border: 1px solid #dbdbdb;
}


QTabWidget {
  padding: 2px;
  selection-background-color: #f7f7f7;
  /* Add wanted borders - fix #141, #126, #123 */
}

QTabWidget  {
  border: 1px solid #f7f7f7;
}

QTabWidget::pane {
  border: 1px solid #f7f7f7;
  border-radius: 4px;
  margin: 0px;
  background-color: #f7f7f7;
  /* Fixes double border inside pane wit pyqt5 */
  padding: 0px;
}

QTabWidget::pane:selected {
  background-color: #f7f7f7;
  border: 1px solid #f7f7f7;
}

QTabBar {
  qproperty-drawBase: 0;
  border-radius: 5px;
  margin: 0px;
  padding: 2px;
  border: 0;
  /* left: 5px; move to the right by 5px - removed for fix */
}

QScrollArea{
  /* fix #159 */
  min-height: 1.25em;
  background-color: #f7f7f7;
  /* fix #159 */
}

QWidget#scrollAreaWidgetContents {
 border-radius: 4px;
  background-color: #f7f7f7;
}

/* QTabBar::tab - selected ------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtabwidget-and-qtabbar

--------------------------------------------------------------------------- */
QTabBar::tab {
  /* !selected and disabled ----------------------------------------- */
  /* selected ------------------------------------------------------- */
}

QTabBar::tab:top:!selected {
  border-bottom: 2px solid #ababab;
  margin-top: 2px;
}

QTabBar::tab:top {
  background-color: #f7f7f7;
  margin-left: 2px;
  padding-left: 4px;
  padding-right: 4px;
  padding-top: 2px;
  padding-bottom: 2px;
  min-width: 5px;
  border-bottom: 3px solid #ababab;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
}

QTabBar::tab:top:selected {
  background-color: #c1d5db;
  border-bottom: 3px solid #68929e;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
}

QTabBar::tab:top:!selected:hover {
  border: 1px solid #c1d5db;
  border-bottom: 3px solid #ababab;
  padding: 0px;
}

QScrollBar:horizontal {
  height: 10px;
  margin: 2px 16px 2px 16px;
  border-radius: 3px;
  background-color: #c4c4c4;
}

QScrollBar:vertical {
background-color: #c4c4c4;
